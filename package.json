{
  "name": "business-management-system",
  "version": "1.0.0",
  "description": "Full-stack Business Management System with PostgreSQL",
  "keywords": [
    "business",
    "management",
    "postgresql",
    "react",
    "express"
  ],
  "license": "MIT",
  "author": "Your Name",
  "scripts": {
    "build": "cd frontend && npm run build",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\"",
